CREATE TABLE PlantEntity (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL
);

selectAll:
SELECT * FROM PlantEntity ORDER BY id;

selectById:
SELECT * FROM PlantEntity WHERE id = ?;

insertItem:
INSERT OR REPLACE INTO PlantEntity (id, name) VALUES (id = ?, name = ?);

deleteById:
DELETE FROM PlantEntity WHERE id = ?;
